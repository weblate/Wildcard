<?xml version="1.0" encoding="UTF-8"?>
<schemalist gettext-domain="wildcard">
	<schema id="@app-id@" path="/io/github/fkinoshita/Wildcard/">

	  <key name="window-width" type="i">
	    <default>415</default>
	    <summary>Window width</summary>
	  </key>

	  <key name="window-height" type="i">
	    <default>570</default>
	    <summary>Window height</summary>
	  </key>

	  <key name="last-regex" type="s">
	    <default>'[A-Z]\\w+'</default>
	    <summary>Last regular expression insert</summary>
	  </key>

	  <key name="last-test" type="s">
	    <default>'Lorem ipsum dolor sit amet.'</default>
	    <summary>Last test insert</summary>
	  </key>

	  <key name="multiline-flag" type="b">
	    <default>true</default>
	    <summary>Use the multiline flag</summary>
	  </key>

	  <key name="case-insensitive-flag" type="b">
	    <default>false</default>
	    <summary>Use the case insensitive flag</summary>
	  </key>

	  <key name="ignore-whitespace-flag" type="b">
	    <default>false</default>
	    <summary>Use the ignore whitespace flag</summary>
	  </key>

	  <key name="dot-matches-newline-flag" type="b">
	    <default>false</default>
	    <summary>Use the dot matches newline flag</summary>
	  </key>

	  <key name="unicode-flag" type="b">
	    <default>false</default>
	    <summary>Use the unicode flag</summary>
	  </key>

	  <key name="greed-flag" type="b">
	    <default>false</default>
	    <summary>Use the greed flag</summary>
	  </key>

	</schema>
</schemalist>
